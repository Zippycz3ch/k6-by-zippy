          "hide": false,
          "query": "from(bucket: \"k6\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r._measurement == \"http_req_duration\")\r\n  |> filter(fn: (r) => r[\"group\"] != \"::setup\")\r\n  |> filter(fn: (r) => r[\"group\"] != \"::teardown\")\r\n  |> filter(fn: (r) => r._field == \"value\")\r\n  |> filter(fn: (r) => r[\"status\"] == \"200\")\r\n  |> filter(fn: (r) => r[\"branch\"] =~ /${branch:regex}/)\r\n  |> filter(fn: (r) => r[\"release\"] =~ /${release:regex}/)\r\n  |> filter(fn: (r) => r[\"buildId\"] =~ /${buildId:regex}/)\r\n  |> filter(fn: (r) => r[\"testid\"] =~ /${testId:regex}/)\r\n  |> filter(fn: (r) => r[\"scenario\"] =~ /${scenario:regex}/)\r\n  |> group(columns: [\"name\"])                    // Group by request name\r\n  |> window(every: 1d)                           // Daily windows\r\n  |> quantile(column: \"_value\", q: 0.9, method: \"exact_mean\")  // P90\r\n  |> duplicate(column: \"_stop\", as: \"_time\")     // Align _time for charting\r\n  |> keep(columns: [\"_time\", \"_value\", \"name\"])\r\n  |> yield(name: \"daily_p90_http_req_duration\")\r\n",
          "refId": "Response time"
        }
      ],
